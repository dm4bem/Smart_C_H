
import numpy as np

T_o = 2
f = open("path to find", 'r')
File = f.read()

Lists = File.split ("\n")
A = []

phi_o = 3

for i in range (2,26) : 
    L = Lists[i].split(",")
    A.append(L[1:20])
C = Lists[26].split(",")[1:20]
Phi = Lists[27].split(",")[1:20]
B = np.zeros(25)
for elt in Lists[28].split(",")[1].split("/") : 
    B[int(elt)] = T_o
for k in range (0,24) :
    for elt in A[k]:
        elt = float(elt)
for char in C :
    if char == "C_Layer out" :
        char = C['Layer_out'] 
    elif char == "C_Layer in" :
        char = C['Layer_in'] 
    elif char == "C_Air" :
        char = C['Air'] 
    elif char == "C_Glass" :
        char = C['Glass'] 
    else : char = float(char)

for char in Phi :
    if char == "phi_oa" :
        char = phi_oa 
    elif char == "phi_ia1" :
        char = phi_ia1 
    elif char == "phi_ia2" :
        char = phi_ia2
    elif char == "phi_ib1" :
        char = phi_ib1
    elif char == "phi_ib2" :
            char = phi_ib2    
    elif char == "phi_ob" :
        char = phi_ob*
    elif char == "phi_a" :
        char = phi_a
    else : char = float(char)
print(Phi)
print (B)
